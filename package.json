{
  "name": "leech",
  "version": "3.1.0",
  "description": "leech cli",
  "homepage": "https://github.com/balena-io/leech#readme",
  "main": "./build/index.js",
  "bin": "./build/index.js",
  "types": "lib/index.ts",
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/balena-io/leech"
  },
  "bugs": {
    "url": "https://github.com/balena-io/leech/issues"
  },
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "postinstall": "gulp build",
    "build": "gulp build",
    "watch": "gulp watch",
    "prepublish": "require-npm4-to-publish",
    "prepublishOnly": "npm run build",
    "prettier-check": "prettier-check --single-quote --write ./lib/*.ts",
    "prettier": "prettier --single-quote --write ./lib/*.ts",
    "test": "mocha -r ts-node/register --reporter spec tests/**/*.spec.ts",
    "lint": "shellcheck scripts/*.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint",
      "pre-push": "npm run prettier-check && npm test"
    }
  },
  "devDependencies": {
    "@types/byline": "4.2.31",
