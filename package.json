    "@types/mocha": "2.2.41",
    "@types/node": "^8.5.9",
    "chai": "4.1.1",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^3.2.4",
    "gulp-util": "^3.0.7",
    "gulp-watch": "4.3.11",
    "husky": "^0.15.0-rc.8",
    "prettier": "^1.5.3",
    "prettier-check": "^2.0.0",
    "require-npm4-to-publish": "^1.0.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.1.5"
