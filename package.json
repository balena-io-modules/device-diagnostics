    "mixpanel": "0.7.0",
    "mkdirp-promise": "5.0.1",
    "mocha": "3.5.0",
    "natives": "^1.1.6",
    "resin-cli-visuals": "1.4.0",
    "resin-token": "4.2.1",
    "shellcheck": "^0.2.3"
